{
  "name": "@nelyo/youtime",
  "version": "1.0.0",
  "description": "Simple timer YouTrack",
  "main": "./dist/main.js",
  "scripts": {
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "dist": "rimraf git-checker-* && electron-packager . --platform=darwin,linux --arch=x64 --icon=app --overwrite --out=packages",
    "build": "tsc"
  },
  "repository": "https://github.com/ElNelyo/youtrack-timer",
  "keywords": [
    "Youtrack",
    "Timer",
    "Work"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^22.3.2",
    "electron-packager": "^14.2.1",
    "tslint": "^5.19.0",
    "typescript": "^3.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "electron-store": "^5.1.1",
    "hacktimer": "^1.1.3",
    "rimraf": "^3.0.2",
    "youtrack-rest-client": "^1.3.2"
  }
}
